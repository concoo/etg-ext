{
  "manifest_version": 3,
  "name": "Text Expander",
  "version": "1.0",
  "description": "Expands predefined short text into full sentences.",
  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
